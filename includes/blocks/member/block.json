        "icons": [
          {
            "icon": "dashicons dashicons-facebook-alt",
            "link": "#"
          },
          {
            "icon": "dashicons dashicons-twitter",
            "link": "#"
          },
          {
            "icon": "dashicons dashicons-linkedin",
           "link": "#"
          }
        ]
      }
    },
    "description": {
      "type": "string"
    }
  },
  "editorScript": "file:./index.js",
  "editorStyle": "file:./build/index.css",
  "style": "file:./build/style-index.css"
